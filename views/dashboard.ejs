<script>
    function strip(str) {
        return str.replace(/^\s+|\s+$/g, '');
    }

    function animateNumber(ele,no,stepTime) {
        $({someValue: 0}).animate({someValue: no}, {
            duration: stepTime,
            step: function () { // called on every step. Update the element's text with value:
                ele.text(Math.floor(this.someValue + 1));
            },
            complete: function () {
                if(no>1000000000)
                    ele.text(no/1000000000 + " Billion");
                else
                    ele.text(no);
            }
        });
    }


    //Detecting Scroll to Section
    var a = 0;
    $(window).scroll(function() {

        var oTop = $('#counter').offset().top - window.innerHeight;
        if (a == 0 && $(window).scrollTop() > oTop) {
            $('.counter-value').each(function() {
                var $this = $(this),
                    countTo = $this.attr('data-count');
                console.log(countTo+"HEMANT");
                console.log(strip($this.text())+'fhwejffew');
                // $this.animateNumber({ number: countTo });
                animateNumber($this,countTo,1500);
            });
            a = 1;
        }

    });

    function setCookie(cname, cvalue, exdays) {
        var d = new Date();
        d.setTime(d.getTime() + (exdays * 24 * 60 * 60 * 1000));
        var expires = "expires="+d.toUTCString();
        document.cookie = cname + "=" + cvalue + ";" + expires + ";path=/";
    }

    function login(){
        var data = {
            uname: document.getElementById("uname").value.toString(),
            password: document.getElementById("password").value.toString()
        };
        console.log(data);
        $.ajax({
            // url: 'http://c2c.rajchandra.me/product/add',
            url: 'http://bd2998ad.ngrok.io/company/login',
            type: 'post',
            // dataType: 'json',
            // contentType: 'application/json',
            data: data,
            success: function (data) {
                console.log(data);
                if(data.code === 0) {
                    alert("Signup Completed");
                    setCookie('c2c',data.data.cookie);
                    window.location.href = '/';
                }
                else{
                    alert("Signup Issue: "+data.message);
                }
            }
        });
    }
</script>
